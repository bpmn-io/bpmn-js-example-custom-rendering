  "license": "MIT",
  "devDependencies": {
    "copy-webpack-plugin": "^8.1.0",
    "eslint": "^7.22.0",
    "eslint-plugin-bpmn-io": "^0.12.0",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "sirv-cli": "^1.0.11",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0",
